{
	"workbench.startupEditor": "none",
	"editor.renderLineHighlight": "gutter",
	"editor.fontFamily": "Hack",
	"editor.fontSize": 14,
	"editor.insertSpaces": true,
	"editor.cursorBlinking": "solid",
	"editor.minimap.renderCharacters": false,
	"editor.rulers": [
    	80
    ],
	"editor.smoothScrolling": false,
	"window.menuBarVisibility": "default",
	"window.restoreWindows": "folders",
	"files.insertFinalNewline": true,
	"window.zoomLevel": 2,
	"editor.minimap.enabled": false,
	"editor.renderControlCharacters": false,
	"workbench.colorCustomizations": {
		"editor.background": "#111",
		"sideBar.background": "#111",
		"terminal.background": "#111",
		"debugToolBar.background": "#111",
		"panel.background": "#111",
		"gitlens.trailingLineForegroundColor": "#777777"
	},
	"editor.tokenColorCustomizations": {
		"comments": "#888888",
	},
	"explorer.confirmDragAndDrop": false,
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": false,
		"suppressShowKeyBindingsNotice": true,
		"suppressUpdateNotice": true,
		"suppressWelcomeNotice": true
	},
	"workbench.activityBar.visible": false,
	"workbench.statusBar.visible": true,
	"zenMode.hideStatusBar": false,
	"zenMode.hideTabs": false,
	"vim.useSystemClipboard": true,
	"workbench.colorTheme": "Dracula",
	"vim.disableExtension": false,
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.enabled": false,
	"gitlens.explorers.avatars": false,
	"gitlens.blame.heatmap.enabled": false,
	"gitlens.keymap": "none",
	"editor.wordWrap": "off",
	"editor.wordWrapColumn": 80,
	"editor.wrappingIndent": "indent",
	"editor.selectionClipboard": false,
	"gitlens.historyExplorer.enabled": false,
	"python.pythonPath": "python3",
	"vsicons.dontShowNewVersionMessage": true,
	"explorer.confirmDelete": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/bin": true,
		"**/obj": true,
		"**/lib": true,
		"**/dist": true,
	},
	"vim.leader": "<space>",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["J"],
			"after": ["m","a","o","<ESC>","`","a"],
		},
		{
			"before": ["K"],
			"after": ["m","a","O","<ESC>","`","a"]
		},
	],
	"files.trimTrailingWhitespace": true,
	"gitlens.gitExplorer.files.layout": "auto",
	"C_Cpp.default.systemIncludePath": [
		"/usr/include",
		"/usr/include/linux",
		"/usr/include/c++/8.2.0",
		"/usr/include/c++/8.2.0/x86_64-solus-linux",
		"/usr/lib64/gcc/x86_64-solus-linux/8.2.0/include",
	],
	"breadcrumbs.enabled": true,
	"editor.cursorStyle": "block",
	"editor.scrollBeyondLastLine": false,
	"workbench.editor.restoreViewState": false,
	"window.titleBarStyle": "custom",
}
