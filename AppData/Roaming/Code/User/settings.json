{
  "workbench.startupEditor": "none",
  "editor.renderLineHighlight": "gutter",
  "editor.fontSize": 16,
  "editor.fontFamily": "'Fira Code', 'FiraCode NF', 'Cascadia Code', Hack, Menlo, 'Font Awesome 5 Brands', 'Font Awesome 5 Free', 'Font Awesome 5 Free Solid', monospace",
  "editor.semanticHighlighting.enabled": true,
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.cursorBlinking": "solid",
  "editor.minimap.renderCharacters": false,
  "editor.rulers": [80],
  "window.restoreWindows": "none",
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.tokenColorCustomizations": {
    "keywords": {
      "fontStyle": "bold"
    },
    "[GitHub Light]": {
      "strings": "#049c04",
      "types": "#282828"
    },
    "[Alabaster]": {
      "types": { "foreground": "#282828" },
      "variables": "#333",
      "strings": "#A00101",
      "numbers": "#026100",
      "textMateRules": [
        {
          "scope": "constant",
          "settings": {
            "foreground": "#A00191",
            "fontStyle": "bold"
          }
        },
        {
          "scope": "variable.other.property",
          "settings": {
            "foreground": "#555"
          }
        },
        {
          "scope": "variable.other.constant",
          "settings": {
            "foreground": "#000DF7"
          }
        }
      ]
    },
    "[Rubber]": {},
    "[The Best Theme]": {
      "comments": "#FF0000"
    }
  },
  "workbench.colorCustomizations": {
    "[Dracula Soft]": {
      "activityBar.background": "#111",
      "editor.background": "#111",
      "titleBar.activeBackground": "#111",
      "titleBar.inactiveBackground": "#000",
      "sideBar.background": "#111"
    },
    "[The Best Theme]": {
      "activityBar.background": "#111",
      "editor.background": "#111",
      "titleBar.activeBackground": "#111",
      "titleBar.inactiveBackground": "#000",
      "sideBar.background": "#111"
    },
    "[Alabaster]": {
      "sideBar.foreground": "#111",
      "activityBar.foreground": "#111"
    }
  },
  "explorer.confirmDragAndDrop": false,
  "workbench.activityBar.visible": true,
  "zenMode.restore": false,
  "vim.useSystemClipboard": true,
  "vim.disableExtension": false,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "indent",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/obj": true
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "after": ["m", "a", "o", "<ESC>", "`", "a"]
    },
    {
      "before": ["K"],
      "after": ["m", "a", "O", "<ESC>", "`", "a"]
    },
    {
      "before": ["leader", "z"],
      "commands": ["editor.foldRecursively"]
    },
    {
      "before": ["leader", "Z"],
      "commands": ["editor.unfoldRecursively"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "f"],
      "commands": ["editor.action.formatDocument"]
    }
  ],
  "files.trimTrailingWhitespace": true,
  "editor.cursorStyle": "block",
  "editor.scrollBeyondLastLine": false,
  "workbench.editor.restoreViewState": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.roundedSelection": false,
  "vim.foldfix": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "scm.diffDecorations": "none",
  "git.openDiffOnClick": true,
  "files.hotExit": "off",
  "workbench.tips.enabled": false,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.replace": "underline",
  "vim.cursorStylePerMode.visualline": "underline",
  "vim.cursorStylePerMode.visualblock": "block-outline",
  "debug.toolBarLocation": "docked",
  "workbench.enableExperiments": false,
  "workbench.colorTheme": "Dracula Soft",
  "workbench.preferredLightColorTheme": "Alabaster",
  "workbench.preferredDarkColorTheme": "Dracula Soft",
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "start",
  "material-icon-theme.hidesExplorerArrows": true,
  "debug.allowBreakpointsEverywhere": true,
  "editor.suggestSelection": "first",
  "files.eol": "\n",
  "diffEditor.codeLens": true,
  "javascript.referencesCodeLens.enabled": true,
  "window.newWindowDimensions": "maximized",
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "github-actions.org-features": true,
  "extensions.ignoreRecommendations": true,
  "editor.tabSize": 2,
  "python.showStartPage": false,
  "python.pythonPath": "C:\\Users\\nathan\\scoop\\shims\\python.exe",
  "python.linting.pylintEnabled": false,
  "python.linting.banditEnabled": true,
  "python.linting.enabled": true,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["-S", "-l 79"],
  "python.languageServer": "Pylance",
  "python.autoComplete.showAdvancedMembers": false,
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.diagnosticMode": "workspace",
  "glassit.alpha": 225,
  "material-icon-theme.activeIconPack": "nest",
  "github-actions.preview-features": true,
  "deno.enable": true,
  "C_Cpp.default.includePath": ["include", "src/include"],
  "vim.autoindent": false,
  "vim.cursorStylePerMode.normal": "block",
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.surround": false,
  "files.associations": {
    "meson.build": "python"
  },
  "editor.showFoldingControls": "always",
  "editor.tabCompletion": "onlySnippets",
  "prettier.semi": false,
  "workbench.sideBar.location": "left",
  "bracketPairColorizer.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    ["Maroon", "Aqua", "Red"],
    "Red"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {},
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.powerShellAdditionalExePaths": [
    {
      "versionName": "7",
      "exePath": "C:\\Users\\nathan\\scoop\\apps\\pwsh\\current\\pwsh.EXE"
    }
  ],
  "powershell.powerShellDefaultVersion": "7",
  "powershell.promptToUpdatePowerShell": false,
  "githubPullRequests.remotes": ["origin", "upstream", "github"],
  "workbench.editor.centeredLayoutAutoResize": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "task.problemMatchers.neverPrompt": {
    "npm": true
  },
  "editor.linkedEditing": true,
  // "svelte.plugin.svelte.format.config.svelteStrictMode": true,
  // "svelte.plugin.css.globals": "**/tokens.css,**/colors.css",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.boxModel": "warning",
  "scss.lint.float": "warning",
  "scss.lint.idSelector": "warning",
  "scss.lint.important": "warning",
  "scss.lint.importStatement": "warning",
  "scss.lint.universalSelector": "warning",
  "scss.lint.unknownVendorSpecificProperties": "warning",
  "scss.lint.zeroUnits": "warning",
  "scss.showErrors": true,
  "prettier.useEditorConfig": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.maxTsServerMemory": 1028,
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "javascript.suggest.completeFunctionCalls": true,
  "workbench.productIconTheme": "fluent-icons",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.showLineNumbers": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "clock.updateInterval": 60000,
  "clock.priority": 60,
  "clock.format": "hh:MM TT",
  "clock.iconName": "clock",
  "clock.alignment": "Right",
  "window.menuBarVisibility": "compact",
  "todo-tree.tree.showScanModeButton": false,
  "go.buildOnSave": "off",
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.lens.references": true,
  "rust-analyzer.debug.engineSettings": {
    "lldb": { "terminal": "integratedTerminal" }
  },
  "docker.showStartPage": false,
  "git.allowNoVerifyCommit": true,
  "git.autoRepositoryDetection": "subFolders",
  "git.fetchOnPull": true,
  "git.pruneOnFetch": true,
  "git.showPushSuccessNotification": true,
  "vim.camelCaseMotion.enable": true,
  "git.enableSmartCommit": true,
  "git.autofetch": false,
  "editor.renderWhitespace": "all",
  "go.lintOnSave": "workspace",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "editor.codeActionsOnSave": null
}
