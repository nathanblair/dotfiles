{
  "suggest.enablePreview": true,
  "python.jediEnabled": false,
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "filetypes": [
        "h",
        "hpp",
        "c",
        "cpp"
      ]
    },
    "dart": {
      "command": "dart",
      "args": ["/opt/dart-sdk/bin/snapshots/analysis_server.dart.snapshot", "--lsp"],
      "disableDynamicRegister": true,
      "filetypes": ["dart"]
    }
  }
}
